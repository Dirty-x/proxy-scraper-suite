{
    "version": 2,
    "builds": [
        {
            "src": "src/bot.ts",
            "use": "@vercel/node"
        }
    ],
    "rewrites": [
        {
            "src": "/api/(.*)",
            "dest": "src/bot.ts"
        },
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        }
    ]
}